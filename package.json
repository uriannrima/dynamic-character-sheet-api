{
  "name": "server",
  "description": "API Server",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Luciano Lima",
    "email": "luciano.lima@ingresso.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "node src/",
    "mocha": "mocha test/ --recursive --exit",
    "dev": "yarn install && nodemon --inspect --watch ../common --watch ../server src/ --exec \"npm run lint && node\"",
    "host": "yarn install && pm2 kill && pm2 start src/ --name 'web-dcs'",
    "lint": "eslint **/*.js",
    "lint:fix": "eslint **/*.js --fix"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "feathers": "^2.2.3",
    "feathers-authentication": "^1.3.1",
    "feathers-authentication-hooks": "^0.1.5",
    "feathers-authentication-jwt": "^0.3.2",
    "feathers-authentication-local": "^0.4.4",
    "feathers-configuration": "^0.4.2",
    "feathers-errors": "^2.9.2",
    "feathers-hooks": "^2.1.2",
    "feathers-hooks-common": "^3.10.0",
    "feathers-memory": "^1.3.1",
    "feathers-mongodb": "^2.9.1",
    "feathers-rest": "^1.8.1",
    "feathers-socketio": "^2.0.1",
    "helmet": "^3.9.0",
    "mongodb": "^2.2.33",
    "nodemon": "^1.12.1",
    "serve-favicon": "^2.4.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "mocha": "^4.0.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  }
}
